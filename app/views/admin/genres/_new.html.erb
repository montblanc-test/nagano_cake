<form class ="form-inline">
  <div class="mr-5">ジャンル名</div>
    <%= form_with model: genre,url:admin_genres_path do |f| %>
      <div class="mr-5"><%= f.text_field :name, autocomplete: "name", class:"form-control",placeholder: 'ジャンル名' %></div>
      <div><%= f.submit '   　新規登録　   ', class: "btn btn-sm btn-success" %></div>
    <% end %>
</form>